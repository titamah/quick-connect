{
    "rewrites": [
      {
        "source": "/remix/:id",
        "destination": "/api/remix/:id"
      },
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/((?!api|_next|favicon\\.ico).*)",
        "destination": "/index.html"
      }
    ]
  }