{
    "functions": {
      "api/remix/[id].js": {
        "runtime": "edge"
      }
    },
    "rewrites": [
      {
        "source": "/remix/:id",
        "destination": "/api/remix/:id"
      },
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/((?!api|_next|favicon\\.ico).*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=0"
          }
        ]
      }
    ]
  }